{
  "name": "sc-prisma-extractor",
  "version": "0.0.1",
  "description": "A CLI tool to extract your Prisma schema into usable TypeScript interfaces and a detailed metadata JSON file.",
  "main": "dist/index.js",
  "bin": {
    "sc-prisma-extractor": "dist/index.js"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "dev": "ts-node src/index.ts ./prisma/schema.prisma ./dev/extracted.ts",
    "prepublishOnly": "npm run build",
    "deploy": "npm run build && npm version patch && npm publish"
  },
  "keywords": [
    "prisma",
    "generator",
    "typescript",
    "interface",
    "cli",
    "schema",
    "extractor"
  ],
  "author": "saulotarsobc",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saulotarsobc/sc-prisma-extractor.git"
  },
  "bugs": {
    "url": "https://github.com/saulotarsobc/sc-prisma-extractor/issues"
  },
  "homepage": "https://github.com/saulotarsobc/sc-prisma-extractor#readme",
  "license": "ISC",
  "dependencies": {
    "@prisma/generator-helper": "^6.14.0",
    "@prisma/internals": "^6.14.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "contributors": [
    {
      "name": "Saulo Costa",
      "email": "saulotarsobc@example.com",
      "url": "https://github.com/saulotarsobc"
    }
  ]
}
